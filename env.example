# Public URL (https://host.example.com)
DOMAIN=https://vault.example.com

# Admin token for /admin (Argon2 PHC string; generate via README steps)
ADMIN_TOKEN=argon2id$example$placeholder$replace_this_with_real_hash

# Absolute host path for persistent data (ZFS or EXT4)
DATA_DIR=/tank/docker/data/vaultwarden

# Behavior and hardening
SIGNUPS_ALLOWED=false
WEBSOCKET_ENABLED=true
LOG_LEVEL=info
TZ=UTC
UID=1000
GID=1000
